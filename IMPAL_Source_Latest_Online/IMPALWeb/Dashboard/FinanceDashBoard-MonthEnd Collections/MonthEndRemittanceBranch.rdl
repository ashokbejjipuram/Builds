        </Field>
        <Field Name="Current_Month_Date">
          <DataField>Current_Month_Date</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Previous_Month_Date">
          <DataField>Previous_Month_Date</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Previous_Year_Date">
          <DataField>Previous_Year_Date</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT        B.Branch_Code, B.Branch_Name, SUM(D.CurrentMonth) / 100000 AS Current_Month_Remittance, SUM(D.PreviousMonth) / 100000 AS Previous_Month_Remittance, 
                         SUM(D.PreviousYear) / 100000 AS Previous_Year_Remittance, SUM(D.CurrentMonth) / 100000 - SUM(D.PreviousMonth) / 100000 AS Diff, D.Current_Month_Date, 
                         D.Previous_Month_Date, D.Previous_Year_Date
FROM            Dash_Remittance AS D INNER JOIN
                         Branch_Master AS B ON B.Branch_Code = D.Branch_Code AND B.State_Code = @State_Code
GROUP BY B.Branch_Code, B.Branch_Name, D.Current_Month_Date, D.Previous_Month_Date, D.Previous_Year_Date
ORDER BY B.Branch_Code</CommandText>
        <QueryParameters>
          <QueryParameter Name="@State_Code">
            <Value>=Parameters!State_Code.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
  </DataSets>
