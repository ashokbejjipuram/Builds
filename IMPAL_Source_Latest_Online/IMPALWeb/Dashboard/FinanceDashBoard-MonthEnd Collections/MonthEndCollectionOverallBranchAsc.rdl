      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT        Zone_Code, Branch_Code, Branch_Name, Zone_Name, SUM(No_of_days_Locking) AS No_of_days_Locking, SUM(CurrentMonth_sales) AS CurrentMonth_sales, 
                         SUM(Optimum_stock) AS Optimum_stock, SUM(Actual_stock) AS Actual_Stock, SUM(OS_PreviousMonth) AS OS_PreviousMonth, SUM(CurrentMonth_Collection) 
                         AS CurrentMonth_Collection, SUM(Collection_Percentage) AS Collection_Percentage, SUM(Bank_balance) AS Bank_balance, Current_Month_Date, 
                         Previous_Month_Date
FROM            (SELECT        DM.Zone_Code, B.Branch_Code, B.Branch_Name, Z.Zone_Name, DM.No_of_days_Locking, DM.CurrentMonth_sales, DM.Optimum_stock, 
                                                    DM.Actual_stock, DM.OS_PreviousMonth, DM.CurrentMonth_Collection, DM.CurrentMonth_Collection / NULLIF (DM.OS_PreviousMonth, 0) 
                                                    * 100 AS Collection_Percentage, DM.Bank_balance, DM.Current_Month_Date, DM.Previous_Month_Date
                          FROM            Dash_MonthEndCollectionZonewise AS DM INNER JOIN
                                                    Branch_Master AS B ON DM.Branch_Code = B.Branch_Code INNER JOIN
                                                    Zone_Master AS Z ON DM.Zone_Code = Z.Zone_Code
                          UNION
                          SELECT        DM.Zone_Code, B.Branch_Code, B.Branch_Name, Z.Zone_Name, 0 AS No_of_days_Locking, 0 AS CurrentMonth_sales, 0 AS Optimum_stock, 
                                                   0 AS Actual_Stock, 0 AS OS_PreviousMonth, 0 AS CurrentMonth_Collection, 0 AS Collection_Percentage, 0 AS Bank_balance, DM.Current_Month_Date, 
                                                   DM.Previous_Month_Date
                          FROM            Dash_MonthEndCollectionZonewise AS DM INNER JOIN
                                                   Branch_Master AS B ON DM.Branch_Code = B.Branch_Code INNER JOIN
                                                   Zone_Master AS Z ON DM.Zone_Code = Z.Zone_Code) AS X
GROUP BY Zone_Code, Branch_Code, Branch_Name, Zone_Name, Current_Month_Date, Previous_Month_Date
ORDER BY Collection_Percentage</CommandText>
      </Query>
    </DataSet>
