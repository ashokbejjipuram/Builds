      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT State_Code, State_Name, SUM(No_of_days_Locking) AS No_of_days_Locking, SUM(CurrentMonth_Sales) AS CurrentMonth_Sales, 
		SUM(PreviousMonth_Sales) AS PreviousMonth_Sales, SUM(OS_CurrentMonth) As OS_CurrentMonth, SUM(OS_PreviousMonth) As OS_PreviousMonth, SUM(OS_PreviousYear) As OS_PreviousYear,
		SUM(No_of_Weeks) As No_of_Weeks, SUM(CurrentMonth_Collection) As CurrentMonth_Collection,  SUM(PreviousMonth_Collection) As PreviousMonth_Collection,
		SUM(PreviousYear_Collection) As PreviousYear_Collection, SUM(CurrentMonth_ColPer) As CurrentMonth_ColPer, 
		SUM(PreviousMonth_ColPer) As PreviousMonth_ColPer, SUM(PrePreviousYear_ColPer) As PrePreviousYear_ColPer,
		SUM(CurrentMonth_CumColPer) As CurrentMonth_CumColPer, SUM(PreviousMonth_CumColPer) As PreviousMonth_CumColPer,
		SUM(PreviousYear_CumColPer) As PreviousYear_CumColPer, CurrentMonth_Date, PreviousMonth_Date, PreviousYear_Date
  FROM (
	SELECT SM.State_Code, SM.State_Name, DMC.No_of_days_Locking, DMC.CurrentMonth_Sales, DMC.PreviousMonth_Sales, DMC.OS_CurrentMonth, 
			DMC.OS_PreviousMonth, DMC.OS_PreviousYear, (DMC.OS_PreviousMonth/NULLIF(DMC.PreviousMonth_Sales,0))*4 AS No_of_Weeks, 
			DMC.CurrentMonth_Collection, DMC.PreviousMonth_Collection, DMC.PreviousYear_Collection, 
			(DMC.CurrentMonth_Collection/NULLIF(DMC.OS_PreviousMonth,0))*100 AS CurrentMonth_ColPer, 
			(DMC.PreviousMonth_Collection/NULLIF(DMC.OS_PrePreviousMonth,0))*100 AS PreviousMonth_ColPer, 
			(DMC.PreviousYear_Collection/NULLIF(DMC.OS_PrePreviousYear,0))*100 AS PrePreviousYear_ColPer, 
			DMC.CurrentMonth_CumColPer, DMC.PreviousMonth_CumColPer, DMC.PreviousYear_CumColPer, DMC.CurrentMonth_Date,
			DMC.PreviousMonth_Date, DMC.PreviousYear_Date  
	   FROM Dash_MonthEndCumulativeCollection DMC
			INNER JOIN State_Master SM on DMC.State_code = SM.State_Code
			AND DMC.Zone_Code = @Zone_Code

	UNION

	SELECT SM.State_Code, SM.State_Name, 0 As No_of_days_Locking, 0 As CurrentMonth_Sales, 0 As PreviousMonth_Sales, 0 As OS_CurrentMonth, 
		   0 As OS_PreviousMonth, 0 As OS_PreviousYear, 0 As No_of_Weeks, 0 As CurrentMonth_Collection, 0 As PreviousMonth_Collection,
		   0 As PreviousYear_Collection, 0 As CurrentMonth_ColPer, 0 As PreviousMonth_ColPer, 0 As PrePreviousYear_ColPer, 
		   0 As CurrentMonth_CumColPer, 0 As PreviousMonth_CumColPer, 0 As PreviousYear_CumColPer,
		   DMC.CurrentMonth_Date, DMC.PreviousMonth_Date, DMC.PreviousYear_Date  
	  FROM Dash_MonthEndCumulativeCollection DMC
			INNER JOIN State_Master SM on DMC.State_code = SM.State_Code
			AND DMC.Zone_Code = @Zone_Code
		)X
GROUP BY State_Code, State_Name, CurrentMonth_Date, PreviousMonth_Date, PreviousYear_Date
Order By CurrentMonth_CumColPer Asc</CommandText>
        <QueryParameters>
          <QueryParameter Name="@Zone_Code">
            <Value>=Parameters!Zone_Code.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
