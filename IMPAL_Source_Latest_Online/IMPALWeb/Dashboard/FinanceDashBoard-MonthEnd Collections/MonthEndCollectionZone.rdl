        <Field Name="Previous_Month_Date">
          <DataField>Previous_Month_Date</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT Zone_code, Zone_Name, SUM(No_of_days_Locking) AS No_of_days_Locking, SUM(CurrentMonth_sales) AS CurrentMonth_sales, 
		SUM(Optimum_stock) AS Optimum_stock, SUM(Actual_Stock) As Actual_Stock, SUM(OS_PreviousMonth) As OS_PreviousMonth, 
		SUM(CurrentMonth_Collection) As CurrentMonth_Collection, SUM(Collection_Percentage) As Collection_Percentage, SUM(Bank_balance) As Bank_balance,
		Current_Month_Date, Previous_Month_Date  
  FROM (
	SELECT Z.Zone_Code, Z.Zone_Name, DM.No_of_days_Locking, DM.CurrentMonth_sales, DM.Optimum_stock, DM.Actual_stock, 
			DM.OS_PreviousMonth, DM.CurrentMonth_Collection, (DM.CurrentMonth_Collection/NULLIF(DM.OS_PreviousMonth,0))*100 AS Collection_Percentage, DM.Bank_balance, 
			DM.Current_Month_Date, DM.Previous_Month_Date 
	   FROM Dash_MonthEndCollectionZonewise DM 
			INNER JOIN Zone_Master Z on DM.Zone_Code = Z.Zone_Code
UNION

	SELECT Z.Zone_Code, Z.Zone_Name, 0 As No_of_days_Locking, 0 As CurrentMonth_sales, 0 As Optimum_stock, 0 As Actual_Stock, 
		   0 As OS_PreviousMonth, 0 As CurrentMonth_Collection, 0 As Collection_Percentage, 0 As Bank_balance,
		   DM.Current_Month_Date, DM.Previous_Month_Date  
	  FROM Dash_MonthEndCollectionZonewise DM 
		   INNER JOIN Zone_Master Z on DM.Zone_Code = Z.Zone_Code
		)X
GROUP BY Zone_Code, Zone_Name, Current_Month_Date, Previous_Month_Date
Order By Zone_Code</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
