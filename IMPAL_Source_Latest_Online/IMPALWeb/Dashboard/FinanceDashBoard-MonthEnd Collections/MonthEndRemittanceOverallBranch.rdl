        <Field Name="Previous_Year_Date">
          <DataField>Previous_Year_Date</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT        B.Branch_Code, B.Branch_Name, Z.Zone_Code, Z.Zone_Name, SUM(D.CurrentMonth) / 100000 AS Current_Month_Remittance, SUM(D.PreviousMonth) 
                         / 100000 AS Previous_Month_Remittance, SUM(D.PreviousYear) / 100000 AS Previous_Year_Remittance, SUM(D.CurrentMonth) / 100000 - SUM(D.PreviousMonth) 
                         / 100000 AS Diff, D.Current_Month_Date, D.Previous_Month_Date, D.Previous_Year_Date
FROM            Dash_Remittance AS D INNER JOIN
                         Branch_Master AS B ON B.Branch_Code = D.Branch_Code INNER JOIN
                         Zone_Master AS Z ON Z.Zone_Code = D.Zone_Code
GROUP BY B.Branch_Code, B.Branch_Name, Z.Zone_Code, Z.Zone_Name, D.Current_Month_Date, D.Previous_Month_Date, D.Previous_Year_Date
ORDER BY Z.Zone_Code</CommandText>
      </Query>
    </DataSet>
  </DataSets>
