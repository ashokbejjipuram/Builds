    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="Zone_Name">
          <DataField>Zone_Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="salesfortheday">
          <DataField>salesfortheday</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="salesaSONDATE">
          <DataField>salesaSONDATE</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>FinanceDS</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT     Z.Zone_Name, SUM(D.Salesfortheday) AS salesfortheday, SUM(D.Salesasondate) AS salesaSONDATE
FROM         Dash_Dlycollection AS D INNER JOIN
                      State_Master AS S ON S.State_Code = S.State_Code INNER JOIN
                      Zone_Master AS Z ON Z.Zone_Code = S.Zone_Code
GROUP BY Z.Zone_Name</CommandText>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
